    <div class="container" style="max-width: 85%">
        <ui-view></ui-view>
    </div>
    <br>
    <br>
    <br>
    <br>
    <br>
    <div ng-class="{'bounceIn': initializeVoiceAlbumComplete, 'fadeOut': initializeVoiceAlbumCompleteOut}" class="animated initializeVoiceAlbum">
        <span class="icon-mic"></span>
        <br>
        <h3><span class="logo">VoiceAlbum</span> turned on</h3>
    </div>
    <div ng-class="{'bounceIn': initializeVoiceAlbumOver, 'fadeOut': initializeVoiceAlbumoverOut}" class="animated initializeVoiceAlbum">
        <span class="icon-mic"></span>
        <br>
        <h3><span class="logo">VoiceAlbum</span> turned off</h3>
    </div>
    <hr>

    <div ng-show="webcamStarted" ng-class="{'bounceIn': webcamStarted}" class="animated webcam" data-drag="true" data-jqyoui-options="{revert2: 'invalid'}" ng-model="list1" jqyoui-draggable="{animate:true}">
        <!-- <webcam channel="myChannel" on-stream="onStream(stream)" on-streaming="onSuccess(data)" on-error="onError(err)"></webcam> -->
        <div id="my_camera"></div>
        <button class="btn btn-default btn-xs" value="Take Snapshot" id="takePhoto">Take photo!</button>
        <div id="results"></div>
