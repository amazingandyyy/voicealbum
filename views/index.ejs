<body ng-app="albumApp" ng-controller="mainCtrl" ng-keypress="keypress($event)">
    <nav class="navbar navbar-default">
        <div class="container" style="max-width: 85%">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" href="#"> VoiceAlbum</a>
            </div>

            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a ui-sref="photos"><span class="icon-pictures"></span></a></li>
                    <li><a ui-sref="photos" ng-click="start()"><span class="icon-quote"><span ng-show="quoteActived" class="quoteActived"></span></span></a></li>
                    <!-- <li><a ui-sref="albums"><span class="icon-layers"></span></a></li> -->
                    <li ng-click="startWebCam()"><a><span class="icon-camera"></span></a></li>
                    <!-- <li><a href="https://github.com/amazingandyyy/album-app" target="_blank"><span class="icon-genius"></span></a></li> -->
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
